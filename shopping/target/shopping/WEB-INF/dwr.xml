<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC
        "-//GetAhead Limited//DTD Direct Web Remoting 3.0//EN"
        "http://getahead.org/dwr/dwr30.dtd">
<dwr>
    <allow>
        <convert converter="bean" match="cn.abovesky.shopping.dwr.*"/>
        <create creator="spring" javascript="MessagePush">
            <param name="beanName" value="MessagePush"/>
        </create>
        <create creator="spring" javascript="Push">
            <param name="beanName" value="Push"/>
        </create>
    </allow>
</dwr>